{
  "name": "karma-benchmark",
  "description": "Continuous JavaScript Performance Monitoring with Benchmark.js and the Karma Runner",
  "version": "0.7.1",
  "author": "Jamie Mason (https://github.com/JamieMason)",
  "bugs": "https://github.com/JamieMason/karma-benchmark/issues",
  "contributors": [
    "Jacob Rask <jacob@jacobrask.net>",
    "Larry Davis <lazdnet@gmail.com>"
  ],
  "dependencies": {
    "benchmark": "2.1.3",
    "gitbook-plugin-anchorjs": "1.1.1",
    "gitbook-plugin-edit-link": "2.0.2",
    "gitbook-plugin-github": "2.0.0",
    "gitbook-plugin-prism": "2.2.0",
    "karma": "1.5.0",
    "lodash": "4.17.4",
    "platform": "1.3.3"
  },
  "devDependencies": {
    "browserify": "14.1.0",
    "gitbook-cli": "2.3.0",
    "jasmine": "2.5.3",
    "jasmine-expect": "3.6.0",
    "xo": "0.17.1"
  },
  "files": [
    "dist/",
    "index.js",
    "src/"
  ],
  "homepage": "https://github.com/JamieMason/karma-benchmark",
  "keywords": [
    "benchmark",
    "jsperf",
    "karma",
    "karma-framework",
    "karma-plugin",
    "monitoring",
    "performance"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": "JamieMason/karma-benchmark",
  "scripts": {
    "build": "browserify src/main.js -o dist/karma-benchmark.js",
    "docs:build": "npm run docs:prepare && rm -rf _book && gitbook build",
    "docs:prepare": "gitbook install",
    "docs:watch": "npm run docs:prepare && gitbook serve",
    "lint": "xo --fix",
    "prebuild": "mkdir -p dist",
    "prepublish": "npm run build",
    "pretest": "npm run build",
    "test": "jasmine JASMINE_CONFIG_PATH=jasmine.json"
  },
  "xo": {
    "envs": [
      "browser",
      "jasmine",
      "node"
    ],
    "globals": [
      "_"
    ],
    "space": 2
  }
}
